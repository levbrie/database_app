<h1>Listing art_movements</h1>

<table>
  <tr>
    <th>Movement</th>
    <th>Title</th>
    <th>Region</th>
    <th>Year start</th>
    <th>Year end</th>
    <th>Time period name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @art_movements.each do |art_movement| %>
  <tr>
    <td><%= art_movement.movement_id %></td>
    <td><%= art_movement.title %></td>
    <td><%= art_movement.region %></td>
    <td><%= art_movement.year_start %></td>
    <td><%= art_movement.year_end %></td>
    <td><%= art_movement.time_period_name %></td>
    <td><%= link_to 'Show', art_movement %></td>
    <td><%= link_to 'Edit', edit_art_movement_path(art_movement) %></td>
    <td><%= link_to 'Destroy', art_movement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

